<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.records.mapper.AuthorityMapper">
    <select id="getAuthorityByInviteCode" resultType="com.example.records.pojo.entity.Authority">
        select *
        from authority
        where invite_code = #{inviteCode}
    </select>
</mapper>